{
  "name": "country-info-app",
  "version": "1.0.0",
  "scripts": {
    "install:all": "npm install --prefix country-info-backend && npm install --prefix country-info-frontend",
    "start:backend": "npm start --prefix country-info-backend",
    "start:frontend": "npm run dev --prefix country-info-frontend",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
